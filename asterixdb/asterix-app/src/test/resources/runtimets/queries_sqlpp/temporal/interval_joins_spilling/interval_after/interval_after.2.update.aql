/*
 * Description      :   Check temporal join spilling functionality for interval after
 * Expected Result  :   Success
 * Date             :   13th Aug, 2020
 */

use dataverse IntervalTest;

create dataset DataSet1(IntervalType) primary key id;
create dataset DataSet2(IntervalType) primary key id;

insert into dataset DataSet1
let $filler := repeat("i", 10000)
let $start := 1
let $end := 100
let $lambda := 1
let $d := 2
let $offset := 0
for $s at $srow in range($start, $end)
for $l at $lrow in range(1, $lambda)
let $id := ($srow-1) * $lambda + $lrow + $offset
return {
"id": $id,
"interval": interval(datetime-from-unix-time-in-secs($s), datetime-from-unix-time-in-secs($s + $d)),
"filler": $filler
};

insert into dataset DataSet2
let $filler := repeat("i", 10000)
let $start := 1
let $end := 100
let $lambda := 1
let $d := 2
let $offset := 0
for $s at $srow in range($start, $end)
for $l at $lrow in range(1, $lambda)
let $id := ($srow-1) * $lambda + $lrow + $offset
return {
"id": $id,
"interval": interval(datetime-from-unix-time-in-secs($s), datetime-from-unix-time-in-secs($s + $d)),
"filler": $filler
};

